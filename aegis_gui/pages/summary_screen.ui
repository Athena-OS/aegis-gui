                            <child>
                              <object class="GtkButton" id="keyboard_button">
                                <property name="valign">center</property>
                                <property name="halign">center</property>
                                <property
                                  name="icon-name">document-edit-symbolic</property>
                              </object>
                            </child>
                          </object>
                        </child>
                        <style>
                          <class name="boxed-list"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
